<%- include("partials/header"); %>


<div class="container" >
<div class="row airfryer" >
    <%  if(airFryerRecipes!=""){%>
        <% airFryerRecipes.forEach(function(recipe) { %>
            <div class="col-lg-4 "> 
            <p><%=recipe.name%></p>
            <a href="/recipes/<%= recipe._id %>"> <img class="square" src="/css/<%= recipe.image %>"/></a>
        </div>
        <%  }) %>
        <%  }  %>
</div>
</div>

<%- include("partials/footer"); %>

